<h1>Locations</h1>
<div class="card-list">
    {% for slug in locations %}
        {% set item = collections.lookup.location[ slug ] %}
        {{ macros.card(
            image    = '/assets/img/location/' + slug + '.webp',
            url      = item.url,
            title    = item.data.title,
            subtitle = "Explore â†’"
        ) }}
    {% endfor %}
</div>

<h1>Monsters</h1>
<div class="card-list">
    {% for slug in monsters %}
        {% set item = collections.lookup.monster[ slug ] %}
        {{ macros.card(
            image    = '/assets/img/monster/' + slug + '.jpg',
            url      = item.url,
            title    = item.data.title,
            subtitle = item.data.species
        ) }}
    {% endfor %}
</div>

<h1>Weapons</h1>
<div class="card-list">
        {% for slug in global_weapons %}
            {% set item = collections.lookup.weapon[ slug ] %}
            {{ macros.card(
                image    = '/assets/img/weapon-character-iceborne/' + slug + '.jpg',
                url      = item.url,
                title    = item.data.title,
                subtitle = item.data.weapon_type
            ) }}
            {% endfor %}
</div>

<h1>Weapons</h1>
<div>
    {% for item in global_weapons %}
        <img
            src="/assets/img/weapon-icon/{{ item | slug }}.png"
            height="40px"
            width="40px"
            style="display:inline-block; border: 1px solid black; padding: 5px; background-color: #333;"
            alt=""
            />
    {% endfor %}
</div>

