<h2>Locations</h2>
<ul>
{% for item in collections.locations %}
    <li
        {% if page.url == item.url %} class="active"{% endif %}>
        <a href="{{ item.url | url }}">
            {{ item.data.title }}
        </a>
        [{{ item.data.title | slug }}] [{{ item.fileSlug }}]
    </li>
{% endfor  %}
</ul>
